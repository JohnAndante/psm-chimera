    "type": "module",
    "scripts": {
        "build": "npx tsc",
        "start": "node dist/index.js",
        "dev": "ts-node src/index.ts",
        "db:generate": "npx prisma generate --schema=./database/prisma/schema.prisma",
        "db:migrate": "prisma migrate deploy --schema=./database/prisma/schema.prisma",
