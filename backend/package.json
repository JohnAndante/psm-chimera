        "db:setup": "npm run db:generate && npm run db:migrate && npm run db:seed"
    },
    "devDependencies": {
        "@types/express": "^5.0.3",
        "@types/node": "^24.5.2",
        "prisma": "^6.16.2",
        "ts-node": "^10.9.2",
